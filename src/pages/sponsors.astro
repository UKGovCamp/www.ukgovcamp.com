---
import BaseLayout from "@/layouts/BaseLayout.astro";
import Sponsor from "../components/Sponsor.astro";
import Image21stCenturyMindset from "../images/21stCenturyMindset.png";
import ImageDxw from "../images/dxw.png";
import ImageFaculty from "../images/Faculty.png";
import ImageGDS from "../images/GDS.png";
import ImageHippo from "../images/hippo.png";
import ImageMadeTech from "../images/MadeTech.png";
import ImageMarvell from "../images/marvell.png";
import ImageNEC from "../images/NECDigitalStudio.png";
import ImageNetcompany from "../images/Netcompany.png";
import ImageNexer from "../images/nexer.png";
import ImageOxfordInsights from "../images/oxfordinsights.png";
import ImagePilotWorks from "../images/PilotWorks.png";
import ImagePublicDigital from "../images/PublicDigital.png";
import ImageRegisterDynamics from "../images/registerdynamics.png";
import ImageScottLogic from "../images/ScottLogic.png";

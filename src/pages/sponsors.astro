import ImageTpxImpact from "../images/tpximpact.png";
---

<BaseLayout title="Our Sponsors">
    <h1>Thank you to our 2025 sponsors!</h1>
    <p>
        Interested in sponsoring the event? Please let us know via the <a
            href="/become-a-sponsor">becoming a sponsor</a
        > page or by contacting us at sponsors@ukgovcamp.com.
    </p>

    <h2>Platinum Sponsors</h2>
    <div class="sponsors">
        <Sponsor
            image={ImageNetcompany}
            name="Netcompany"
            title="Lunch"
            link="https://netcompany.com/"
        />
        <Sponsor
            image={ImageOxfordInsights}
            name="Oxford Insights"
            title="CrÃ¨che"
            link="https://www.oxfordinsights.com/"
        />
        <Sponsor
            image={ImageTpxImpact}
            name="TPXimpact"
            title="Coffee & Tea"
            link="https://www.tpximpact.com/"
        />
        <Sponsor
            image={ImageMadeTech}
            name="Made Tech"
            title="Bursaries"
            link="https://www.madetech.com/"
        />
        <Sponsor
            image={ImageDxw}
            name="dxw"
            title="Accessibility"
            link="https://www.dxw.com/"
        />
        <Sponsor
            image={ImageScottLogic}
            name="Scott Logic"
            title="Carbon Offsetting"
            link="https://www.scottlogic.com/"
        />
    </div>

    <h2>Gold sponsors</h2>
    <div class="sponsors">
        <Sponsor
            image={ImageFaculty}
            name="Faculty"
            link="https://faculty.ai/"
        />
        <Sponsor
            image={ImagePublicDigital}
            name="Public Digital"
            link="https://public.digital/"
        />
        <Sponsor
            image={ImageNEC}
            name="NEC Digital Studio"
            link="https://necsws.com/digitalstudio/"
        />
        <Sponsor
            image={ImageGDS}

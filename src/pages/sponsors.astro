        />
    </div>

    <h2>Silver sponsors</h2>
    <div class="sponsors">
        <Sponsor
            image={ImageNexer}
            name="Nexer"
            link="https://nexergroup.com/"
        />
        <Sponsor
            image={ImageHippo}
            name="Hippo"
            link="https://hippodigital.co.uk/"
        />
        <Sponsor
            image={ImageSlack}
            name="Slack"

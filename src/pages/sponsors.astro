---
import BaseLayout from "@/layouts/BaseLayout.astro";
import Sponsor from "../components/Sponsor.astro";
import Image21stCenturyMindset from "../images/21stCenturyMindset.png";
import ImageDxw from "../images/dxw.png";
import ImageFaculty from "../images/Faculty.png";
import ImageGDS from "../images/GDS.png";
import ImageHippo from "../images/hippo.png";
import ImageMadeTech from "../images/MadeTech.png";
import ImageMarvell from "../images/marvell.png";
import ImageNEC from "../images/NECDigitalStudio.png";
import ImageNetcompany from "../images/Netcompany.png";
import ImageNexer from "../images/nexer.png";
import ImageOxfordInsights from "../images/oxfordinsights.png";
import ImagePilotWorks from "../images/PilotWorks.png";
import ImagePublicDigital from "../images/PublicDigital.png";
import ImageRegisterDynamics from "../images/registerdynamics.png";
import ImageScottLogic from "../images/ScottLogic.png";
import ImageSlack from "../images/slack.png";
import ImageTpxImpact from "../images/tpximpact.png";
---

<BaseLayout title="Our Sponsors">
    <h1>UKGovCamp 2026 (#ukgc26) sponsors</h1>
    <p>
        Interested in sponsoring? Please visit our <a
            href="/become-a-sponsor">become a sponsor</a
        > page.
    </p>

    <h2>Diamond Sponsors</h2>
    <div class="sponsors">
        <Sponsor
            image={Jadu}
            name="Jadu"
            title=""
            link="https://www.jadu.net/"
        />
    </div>
    <div class="sponsors">
        <Sponsor
            image={Softwire}
            name="Softwire"
            title=""
            link="https://netcompany.com/"
        />
    </div>

    <h2>Platinum Sponsors</h2>
    <div class="sponsors">
        <Sponsor
            image={Dxw}
            name="Dxw"
            link="https://www.dxw.com/"
        />
    </div>
    <div class="sponsors">
        <Sponsor
            image={Methods}
            name="Methods"
            link="https://www.methods.co.uk/"
        />
    </div>
    <div class="sponsors">
        <Sponsor
            image={SocietyWorks}
            name="SocietyWorks"
            link="https://www.societyworks.org/"
        />
    </div>

    <h2>Gold sponsors</h2>
    <div class="sponsors">
        <Sponsor
            image={}
            name=""
            link=""
        />
    </div>

    <h2>Silver sponsors</h2>
    <div class="sponsors">
        <Sponsor
            image={}
            name=""
            link=""
        />
    </div>

    <h2>Bronze sponsors</h2>
    <div class="sponsors">
        <Sponsor
            image={Paper}
            name="Paper"
            link="https://paper.studio/"
        />
    </div>

</BaseLayout>

    <h2>Bronze sponsors</h2>
    <div class="sponsors">
        <Sponsor
            image={Image21stCenturyMindset}
            name="21st Century Mindset"
            link="https://21cmindset.com/"
        />
        <Sponsor
            image={ImageMarvell}
            name="Marvell Consulting"
            link="https://marvell-consulting.com/"
        />
        <Sponsor
            image={ImageRegisterDynamics}
            name="Register Dynamics"
            link="https://www.register-dynamics.co.uk/"
        />
    </div>

    <h2>Other awesome supports</h2>
    <p>Finance and Seed Fund Management &mdash; James Cattell</p>
    <p>Finance Administration &mdash; Vanessa Montgomery Williams</p>
</BaseLayout>

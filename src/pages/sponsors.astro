---
import BaseLayout from "@/layouts/BaseLayout.astro";
import Sponsor from "../components/Sponsor.astro";

import ImageCaci from "../images/sponsors/caci.png";
import ImageDelib from "../images/sponsors/delib.png";
import ImageDxw from "../images/sponsors/dxw.png";
import ImageFaculty from "../images/sponsors/faculty.png";
import ImageGain from "../images/sponsors/gain.jpg";
import ImageHippoDigital from "../images/sponsors/hippo.svg";
import ImageJadu from "../images/sponsors/jadu.png";
import ImageMadeTech from "../images/sponsors/madetech.png";
import ImageMethods from "../images/sponsors/methods.svg";
import ImageNECDigitalStudio from "../images/sponsors/necdigitalstudio.png";
import ImageNowPrototypeIT from "../images/sponsors/nowprototypeit.jpg";
import ImageOxfordInsights from "../images/sponsors/oxfordinsights.png";
import ImagePaper from "../images/sponsors/paper.png";
import ImagePublicDigital from "../images/sponsors/publicdigital.jpg";
import ImageRegisterDynamics from "../images/sponsors/registerdynamics.png";
import ImageSocietyWorks from "../images/sponsors/societyworks.png";
import ImageSoftwire from "../images/sponsors/softwire.jpg";
import ImageSwiftStrategies from "../images/sponsors/swift.png";
import ImageTorchbox from "../images/sponsors/torchbox.png";
import ImageTPXImpact from "../images/sponsors/tpximpact.png";
import ImageWebcurl from "../images/sponsors/webcurl.svg";
---

<BaseLayout title="Our Sponsors">
    <h1>UKGovCamp 2026 (#ukgc26) sponsors</h1>
    <p>
        Interested in sponsoring? Please visit our <a
            href="/become-a-sponsor">become a sponsor</a
        > page.
    </p>

    <h2>Diamond Sponsors</h2>
    <div class="sponsors">
        <Sponsor
            image={ImageCaci}
            name="CACI "
            link="https://www.caci.co.uk/"
        />
        <Sponsor
            image={ImageFaculty}
            name="Faculty"
            link="https://faculty.ai/"
        />
        <Sponsor
            image={ImageGain}
            name="GAIN"
            link="https://www.thisisgain.com/"
        />
        <Sponsor
            image={ImageJadu}
            name="Jadu"
            link="https://www.jadu.net/"
        />
        <Sponsor
            image={ImageMadeTech}
            name="Made Tech"
            link="https://www.madetech.com/"
        />
        <Sponsor
            image={ImageNowPrototypeIT}
            name="Now Prototype IT"
            link="https://nowprototype.it/"
        />
        <Sponsor
            image={ImageOxfordInsights}
            name="Oxford Insights"
            link="https://oxfordinsights.com/"
        />
        <Sponsor
            image={ImageRegisterDynamics}
            name="Register Dynamics"
            link="https://www.register-dynamics.co.uk/"
        />
        <Sponsor
            image={ImageSoftwire}
            name="Softwire"
            link="https://www.softwire.com/"
        />
    </div>

    <h2>Platinum Sponsors</h2>
    <div class="sponsors">
        <Sponsor
            image={ImageDxw}
            name="Dxw"
            link="https://www.dxw.com/"
        />
        <Sponsor
            image={ImageMethods}
            name="Methods"
            link="https://www.methods.co.uk/"
        />
        <Sponsor
            image={ImageSocietyWorks}
            name="SocietyWorks"
            link="https://www.societyworks.org/"
        />
        <Sponsor
            image={ImageWebcurl}
            name="Webcurl"
            link="https://www.webcurl.co.uk/"
        />
    </div>

    <h2>Gold sponsors</h2>
    <div class="sponsors">
        <Sponsor
            image={ImageDelib}
            name="Delib"
            link="https://www.delib.net/"
        />
        <Sponsor
            image={ImageHippoDigital}
            name="Hippo Digital"
            link="https://hippodigital.co.uk/"
        />
        <Sponsor
            image={ImageNECDigitalStudio}
            name="NEC Digital Studio"
            link="https://necsws.com/digitalstudio/"
        />
        <Sponsor
            image={ImagePublicDigital}
            name="Public Digital"
            link="https://public.digital/"
        />
        <Sponsor
            image={ImageSwiftStrategies}
            name="Swift Strategies"
            link="https://swiftstrategies.co.uk/"
        />
        <Sponsor
            image={ImageTorchbox}
            name="Torchbox"
            link="https://torchbox.com/"
        />
        <Sponsor
            image={ImageTPXImpact}
            name="TPX Impact"
            link="https://www.tpximpact.com/"
        />
    </div>

    <h2>Bronze sponsors</h2>
    <div class="sponsors">
        <Sponsor
            image={ImagePaper}
            name="Paper"
            link="https://paper.studio/"
        />
    </div>
</BaseLayout>
